{
  "name": "mini_kintai",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install && cd FE && npm ci --include=dev && npm run build && cd .. && rm -rf public && cp -r FE/dist public && npm run db:migrate && npm run db:seed",
    "db:migrate": "npx knex migrate:latest --knexfile ./db/knexfile.js",
    "db:migrate-make": "npx knex migrate:make --knexfile ./db/knexfile.js",
    "db:seed": "npx knex seed:run --knexfile ./db/knexfile.js",
    "db:seed-make": "npx knex seed:make --knexfile ./db/knexfile.js",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
